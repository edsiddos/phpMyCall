{
  "name": "openMyCall",
  "authors": [
    "Ednei Leite da Silva <edsiddos@gmail.com>"
  ],
  "description": "Sistema de Helpdesk para controle de chamados desenvolvido em PHP",
  "main": "",
  "moduleType": [],
  "keywords": [
    "Helpdesk",
    "controle",
    "de",
    "chamados"
  ],
  "license": "GPLv3",
  "homepage": "",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "~3.3.6",
    "font-awesome": "~4.5.0",
    "bootstrap-select": "~1.9.3",
    "awesome-bootstrap-checkbox": "~0.3.5",
    "jquery-mask-plugin": "jQuery-Mask-Plugin#~1.13.4",
    "bootstrap-table": "^1.10.1"
  },
  "install": {
    "base": "static",
    "path": {
      "js": "{name}/js",
      "css": "{name}/css",
      "eot": "{name}/fonts",
      "otf": "{name}/fonts",
      "svg": "{name}/fonts",
      "ttf": "{name}/fonts",
      "woff": "{name}/fonts",
      "woff2": "{name}/fonts"
    },
    "sources": {
      "bootstrap": [
        "bower_components/bootstrap/dist/css/bootstrap.min.css",
        "bower_components/bootstrap/dist/js/bootstrap.min.js",
        "bower_components/bootstrap/dist/fonts/*"
      ],
      "bootstrap-table": [
        "bower_components/bootstrap-table/dist/bootstrap-table.min.css",
        "bower_components/bootstrap-table/dist/bootstrap-table.min.js",
        "bower_components/bootstrap-table/dist/bootstrap-table-locale-all.min.js"
      ],
      "font-awesome": [
        "bower_components/font-awesome/css/font-awesome.min.css",
        "bower_components/font-awesome/fonts/*"
      ],
      "awesome-bootstrap-checkbox": [
        "bower_components/awesome-bootstrap-checkbox/awesome-bootstrap-checkbox.css"
      ],
      "bootstrap-select": [
        "bower_components/bootstrap-select/dist/css/bootstrap-select.min.css",
        "bower_components/bootstrap-select/dist/js/bootstrap-select.min.js",
        "bower_components/bootstrap-select/dist/js/i18n/defaults-pt_BR.min.js"
      ],
      "jquery": [
        "bower_components/jquery/dist/jquery.min.js"
      ],
      "jquery-mask-plugin": [
        "bower_components/jquery-mask-plugin/dist/jquery.mask.min.js"
      ]
    }
  }
}
